{
  "name": "jonar-code-challenge",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all format lint test clean && tsc",
    "start": "node build/src",
    "dev": "ts-node src",
    "dev:watch": "nodemon src -e ts,json --exec 'npm run dev'",
    "lint": "eslint src/*.ts",
    "format": "npx prettier --write src",
    "format:check": "npx prettier --check src",
    "test": "jest"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^16.18.25",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  }
}
